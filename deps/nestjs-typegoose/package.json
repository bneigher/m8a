{
  "name": "@m8a/nestjs-typegoose",
  "version": "8.1.0",
  "description": "A nestjs module wrapper for typegoose",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m8a-io/m8a.git",
    "directory": "deps/nestjs-typegoose"
  },
  "license": "MIT",
  "author": "Kyle Pfromer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "commit": "git cz",
    "coverage": "yarn run test:cov && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint --project tsconfig.json",
    "prepare": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "resetMocks": true,
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "is-class": "0.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.0",
    "@commitlint/config-conventional": "17.4.0",
    "@nestjs/common":"9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/testing": "9.2.0",
    "@types/jest": "29.2.5",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "coveralls": "3.1.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^39.3.21",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "mongodb-memory-server": "8.11.0",
    "mongoose": "6.8.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "superagent": "8.0.6",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "@typegoose/typegoose": "10.0.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@typegoose/typegoose": "^9.0.0 || ^10.0.0",
    "mongoose": "^6.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "har-validator": "5",
      "uuid": "3",
      "superagent": "*"
    }
  }
}
